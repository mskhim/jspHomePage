function openLoginPopup() {
    const width = 100;
    const height = 300;
    const left = (window.innerWidth - width) / 2;
    const top = (window.innerHeight - height) / 2;
    window.open(
        "/jspHomePage/home/loginPopupPage.jsp",
        "로그인",
        `width=${width},height=${height},resizable=no,scrollbars=no`
    );
}